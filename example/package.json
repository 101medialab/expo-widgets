{
  "name": "expo-widgets-example",
  "version": "2.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "prebuild:ios": "EXPO_NO_CACHE=true EXPO_NO_GIT_STATUS=true expo prebuild -p ios --clean",
    "prebuild:android": "EXPO_NO_CACHE=true EXPO_NO_GIT_STATUS=true expo prebuild -p android --clean"
  },
  "dependencies": {
    "@bittingz/expo-widgets": "^0.7.1",
    "expo": "^51.0.17",
    "expo-dev-client": "~4.0.19",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-native": "0.74.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/react": "~18.2.79",
    "typescript": "^5.3.0",
    "xcode": "^3.0.1"
  },
  "private": false,
  "expo": {
    "autolinking": {
      "nativeModulesDir": ".."
    }
  }
}
