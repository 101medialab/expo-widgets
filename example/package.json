{
  "name": "expo-widgets-example",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "prebuild:ios": "EXPO_NO_CACHE=true EXPO_NO_GIT_STATUS=true expo prebuild -p ios --clean",
    "prebuild:android": "EXPO_NO_CACHE=true EXPO_NO_GIT_STATUS=true expo prebuild -p android --clean"
  },
  "dependencies": {
    "@bittingz/expo-widgets": "^0.5.55",
    "expo": "^50.0.5",
    "expo-dev-client": "~3.3.8",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.73.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "typescript": "^5.3.0",
    "xcode": "^3.0.1"
  },
  "private": false,
  "expo": {
    "autolinking": {
      "nativeModulesDir": ".."
    }
  }
}
